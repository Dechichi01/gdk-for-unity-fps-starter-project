{
  "build": {
  },
  "bridge": {
    "worker_attribute_set": {
      "attributes": [
        "SimulatedPlayer"
      ]
    },
    "entity_interest": {
      "range_entity_interest": {
        "radius": 60
      }
    },
    "component_delivery": {
      "default": "RELIABLE_ORDERED",
      "override": {
        "improbable.Position": {
          "checkout_initially": true
        },
        "improbable.Metadata": {
          "checkout_initially": true
        },
        "improbable.Persistence": {
          "checkout_initially": true
        },
        "improbable.EntityAcl": {
          "checkout_initially": true
        },
        "improbable.gdk.movement.ServerMovement": {
          "checkout_initially": true
        },
        "improbable.gdk.guns.ShootingComponent": {
          "checkout_initially": true
        },
        "improbable.gdk.guns.GunStateComponent": {
          "checkout_initially": true
        },
        "improbable.gdk.guns.GunComponent": {
          "checkout_initially": true
        },
        "improbable.player_lifecycle.PlayerHeartbeatClient": {
          "checkout_initially": true
        },
        "improbable.player_lifecycle.PlayerHeartbeatServer": {
          "checkout_initially": true
        },
        "improbable.player_lifecycle.OwningWorker": {
          "checkout_initially": true
        },
        "improbable.gdk.health.HealthComponent": {
          "checkout_initially": true
        },
        "improbable.gdk.health.HealthRegenComponent": {
          "checkout_initially": true
        }
      }
    }
  },
  "external": {
    "default": {
      "run_type": "EXECUTABLE",
      "windows": {
        "command": "build/worker/SimulatedPlayer@Windows/SimulatedPlayer@Windows.exe",
        "arguments": [
          "+workerType",
          "SimulatedPlayer",
          "+infraServicesUrl",
          "http://127.0.0.1:21000",
          "+projectName",
          "${IMPROBABLE_PROJECT_NAME}",
          "-logfile",
          "../../logs/external-default-simulatedplayer.log"
        ]
      },
      "macos": {
        "command": "open",
        "arguments": [
          "./build/worker/SimulatedPlayer@Mac/SimulatedPlayer@Mac.app",
          "--args",
          "+assemblyName",
          "local_assembly",
          "+workerType",
          "SimulatedPlayer",
          "+infraServicesUrl",
          "http://127.0.0.1:21000",
          "+projectName",
          "${IMPROBABLE_PROJECT_NAME}",
          "-logfile",
          "../../logs/external-default-simulatedplayer.log"
        ]
      }
    }
  }
}
