{
  "build": {
    "tasks": [
      {
        "name": "Codegen",
        "steps": [
          {
            "name": "** Load Unity to generate code **",
            "command": "echo",
            "arguments": [ "No-op." ]
          }
        ]
      },
      {
        "name": "build",
        "steps": [
          {
            "name": "** Run ci/build-gamelogic.sh instead **",
            "command": "echo",
            "arguments": [ "No-op." ]
          }
        ]
      },
      {
        "name": "clean",
        "steps": [
          {
            "name": "No-op",
            "command": "echo",
            "arguments": [ "No-op." ]
          }
        ]
      }
    ]
  },
  "bridge": {
    "worker_attribute_set": {
      "attributes": [
        "DeploymentManager"
      ]
    },
    "entity_interest": {
      "range_entity_interest": {
        "radius": 500
      }
    },
    "component_delivery": {
      "default": "RELIABLE_ORDERED",
      "checkoutAllInitially": true
    }
  },
  "managed": {
    "linux": {
      "artifact_name": "DeploymentManager@Linux.zip",
      "command": "DeploymentManager",
      "arguments": [
        "--receptionist-host",
        "${IMPROBABLE_RECEPTIONIST_HOST}",
        "--receptionist-port",
        "${IMPROBABLE_RECEPTIONIST_PORT}",
        "--worker-id",
        "${IMPROBABLE_WORKER_ID}",
        "--log-file",
        "${IMPROBABLE_LOG_FILE}",
        "--project-name",
        "${IMPROBABLE_PROJECT_NAME}",
        "--assembly-name",
        "${IMPROBABLE_ASSEMBLY_NAME}",
        "--player-type",
        "UnityClient",
        "--deployment-prefix",
        "session",
        "--number-of-deployments",
        "1",
        "--max-players",
        "4"
      ]
    },
    "windows": {
      "artifact_name": "DeploymentManager@Windows.zip",
      "command": "DeploymentManager.exe",
      "arguments": [
        "--receptionist-host",
        "${IMPROBABLE_RECEPTIONIST_HOST}",
        "--receptionist-port",
        "${IMPROBABLE_RECEPTIONIST_PORT}",
        "--worker-id",
        "${IMPROBABLE_WORKER_ID}",
        "--log-file",
        "${IMPROBABLE_LOG_FILE}",
        "--project-name",
        "${IMPROBABLE_PROJECT_NAME}",
        "--assembly-name",
        "${IMPROBABLE_ASSEMBLY_NAME}",
        "--player-type",
        "UnityClient",
        "--deployment-prefix",
        "session",
        "--number-of-deployments",
        "1",
        "--max-players",
        "4"
      ]
    }
  }
}
